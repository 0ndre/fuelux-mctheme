<!doctype HTML>
<html>
<head>
	<title>Fuel UX 3 Icons</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<link href="/bower_components/bootstrap/dist/css/bootstrap.css" rel="stylesheet" type="text/css">
	<link href="/bower_components/fuelux/dist/css/fuelux.css" rel="stylesheet" type="text/css">

	<link href="/dist/css/fuelux-mctheme.css" rel="stylesheet" type="text/css">
	<link href="/examples/icons/fuelux-icon-example.css" rel="stylesheet" type="text/css">
<script>
	{{{loaderText}}}
	{{#with cssFiles}}
	grunticon(["{{svg}}", "{{png}}", "{{fallback}}"]);
	</script>
		<noscript>
			<link href="{{fallback}}" rel="stylesheet">
		</noscript>
	{{/with}}
</head>
<body>
<div class="container">
	<div class="row">
		<div class="col-md-12">
			<ul class="fuelux-icon-list">
				{{#each icons}}
					{{#with this}}
						<li>
							<div class="fuelux-icon fuelux-icon-{{name}}"></div>
							<code>.fuelux-icon {{prefix}}{{name}}</code>
						</li>
					{{/with}}
				{{/each}}
			</ul>
		</div>
	</div>
</div>
</body>
</html>
